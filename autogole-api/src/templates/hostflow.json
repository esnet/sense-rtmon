          "expr": "irate(node_network_transmit_errs_total{instance=~\"REPLACEME_HOSTNAME.*\",sitename=\"REPLACEME_SITENAME\", device=~\"REPLACEME_INTERFACE\"}[2m])",
          "format": "time_series",
          "intervalFactor": 2,
          "legendFormat": "{{device}} - Rransmit errors",
          "refId": "B",
